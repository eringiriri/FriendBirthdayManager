    <sys:String x:Key="DialogTitleCsvImportResult">CSV インポート結果</sys:String>
    <sys:String x:Key="DialogTitleCsvImportComplete">CSV インポート完了</sys:String>
    <sys:String x:Key="DialogTitleError">エラー</sys:String>
    <sys:String x:Key="DialogTitleConfirmDelete">削除確認</sys:String>

