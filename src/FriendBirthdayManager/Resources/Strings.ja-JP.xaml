    <sys:String x:Key="ButtonExport">エクスポート</sys:String>
    <sys:String x:Key="SortByNearest">近い順</sys:String>
    <sys:String x:Key="SortByDate">日付順</sys:String>
    <sys:String x:Key="SortByName">名前順</sys:String>
