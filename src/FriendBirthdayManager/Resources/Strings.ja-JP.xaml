    <sys:String x:Key="YearSuffix">年</sys:String>
    <sys:String x:Key="MonthSuffix">月</sys:String>
    <sys:String x:Key="DaySuffix">日</sys:String>
    <sys:String x:Key="NotSet">未設定</sys:String>
    <sys:String x:Key="DaysUntilNone">－</sys:String>

